{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "allowJs": true,
        "checkJs": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@components/*": ["src/components/*"],
            "@utils/*": ["src/utils/*"],
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*"]
}